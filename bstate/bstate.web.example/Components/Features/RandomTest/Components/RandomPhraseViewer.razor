@inherits bstate.core.Components.BStateComponent

<h3>RandomPhraseViewer</h3>
<h4>@State.Name</h4>

@code {
    RandomTestState State => this.UseState<RandomTestState>();

    protected override void ConfigureCustomLifeCycle()
    {
        this.UseOnInitiaze<TestCustomLifeCycle>();
        this.UseOnAfterRenderAsync<TestCustomLifeCycle>();
        this.UseOnBStateRender<TestCustomLifeCycle>();
    }

}